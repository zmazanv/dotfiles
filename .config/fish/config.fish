# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
###############################################################################
# ~/.config/fish/config.fish
###############################################################################
#------------------------------------------------------------------------------
# Si no se está ejecutando interactivamente, no se hará nada.
#------------------------------------------------------------------------------
# If not running interactively, nothing will be done.
#------------------------------------------------------------------------------
if status is-interactive
#------------------------------------------------------------------------------
###############################################################################

    ###############################################################################
    # ALIAS
    ###############################################################################
    # ALIASES
    ###############################################################################
    #------------------------------------------------------------------------------
    # Para Aplicaciones.
    #------------------------------------------------------------------------------
    # For Applications.
    #------------------------------------------------------------------------------
    source ~/.config/fish/conf.d/alias/applications.fish
    #------------------------------------------------------------------------------
    #
    #------------------------------------------------------------------------------
    # Para Repositorios de Git.
    #------------------------------------------------------------------------------
    # For Git Repositories.
    #------------------------------------------------------------------------------
    source ~/.config/fish/conf.d/alias/git.fish
    #------------------------------------------------------------------------------
    ###############################################################################

    ###############################################################################
    # MAPAS
    ###############################################################################
    # BINDS
    ###############################################################################
    #------------------------------------------------------------------------------
    # Mapas.
    #------------------------------------------------------------------------------
    # Binds.
    #------------------------------------------------------------------------------
    source ~/.config/fish/conf.d/binds/binds.fish
    #------------------------------------------------------------------------------
    ###############################################################################

    ###############################################################################
    # INICIO
    ###############################################################################
    # STARTUP
    ###############################################################################
    #------------------------------------------------------------------------------
    # Aplicaciones.
    #------------------------------------------------------------------------------
    # Applications.
    #------------------------------------------------------------------------------
    source ~/.config/fish/conf.d/startup/applications.fish
    #------------------------------------------------------------------------------
    #
    #------------------------------------------------------------------------------
    # Prompt.
    #------------------------------------------------------------------------------
    source ~/.config/fish/conf.d/startup/prompt.fish
    #------------------------------------------------------------------------------
    ###############################################################################
###############################################################################
end
###############################################################################
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
