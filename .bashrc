# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
###############################################################################
# $HOME/.bashrc
###############################################################################
#------------------------------------------------------------------------------
# Si no se está ejecutando interactivamente, no se hará nada.
#------------------------------------------------------------------------------
# If not running interactively, nothing will be done.
#------------------------------------------------------------------------------
[[ $- != *i* ]] && return
#------------------------------------------------------------------------------
###############################################################################
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

###############################################################################
# FUNCIONES
###############################################################################
# FUNCTIONS
###############################################################################
source $HOME/.bash.d/rc.d/functions/functions.sh
###############################################################################

###############################################################################
# ALIAS
###############################################################################
# ALIASES
###############################################################################
#------------------------------------------------------------------------------
# Para Aplicaciones.
#------------------------------------------------------------------------------
# For Applications.
#------------------------------------------------------------------------------
source $HOME/.bash.d/rc.d/alias/applications.sh
#------------------------------------------------------------------------------
#
#------------------------------------------------------------------------------
# Para Usos Misceláneos.
#------------------------------------------------------------------------------
# For Miscellanous Uses.
#------------------------------------------------------------------------------
source $HOME/.bash.d/rc.d/alias/miscellaneous.sh
#------------------------------------------------------------------------------
#
#------------------------------------------------------------------------------
# Para Repositorios de Git.
#------------------------------------------------------------------------------
# For Git Repositories.
#------------------------------------------------------------------------------
source $HOME/.bash.d/rc.d/alias/git.sh
#------------------------------------------------------------------------------
###############################################################################

###############################################################################
# INICIO
###############################################################################
# STARTUP
###############################################################################
#------------------------------------------------------------------------------
# Aplicaciones.
#------------------------------------------------------------------------------
# Applications.
#------------------------------------------------------------------------------
source $HOME/.bash.d/rc.d/startup/applications.sh
#------------------------------------------------------------------------------
#
#------------------------------------------------------------------------------
# Prompt.
#------------------------------------------------------------------------------
source $HOME/.bash.d/rc.d/startup/prompt.sh
#------------------------------------------------------------------------------
###############################################################################
